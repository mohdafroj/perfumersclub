
/* start of header_section css */
.rowSectionDiv{position:relative; padding:0;}
.rowSectionDiv a, .rowSectionDiv .moreIfoText{background:#3b4e76; font-size:1.2rem; color:#ffffff; border:.1rem solid transparent; text-transform:capitalize; padding:.8rem 1rem; display:block; text-align:center; position:relative; border-radius:0; transition:ease all .5s; -webkit-transition:ease all .5s;}
.rowSectionDiv + .rowSectionDiv a, .rowSectionDiv + .rowSectionDiv .moreIfoText{border-left:.01rem solid rgba(255,255,255,.3);}
.rowSectionDiv a.logoLink{background:#ffffff; padding:0;}
.rowSectionDiv a.logoLink span{position:absolute; top:-1.5rem; left: 2rem; z-index:1;}
.rowSectionDiv a.logoLink img{width:3.5rem;}
.rowSectionDiv a:hover{color:#ee9591;}
.rowSectionDiv a.active{border-color: #ee9591; color:#ee9591;}
.rowSectionDiv a.active:before{content:"\f0d7"; font-family:"Font Awesome 5 Free"; font-weight:bold; position:absolute; bottom:-1.5rem; top:auto; left:50%; transform:translateX(-50%); font-size:1.8rem; color:#3b4e76;}


.rowSectionDiv a.categoryDiv.active:after{content:"\f0d8"; font-family:"Font Awesome 5 Free"; font-weight:bold; position:absolute; top:-1.6rem; bottom:auto; left:50%; transform:translateX(-50%); font-size:1.8rem; color:#3b4e76;}

/************* start of footer_section css **********/
.footerDiv{position:fixed; bottom:0; left:0; width:100%; z-index: 9;}
.rowSectionDiv a.addCartText{background:#ee9591; color:#3b4e76;}
.rowSectionDiv + .rowSectionDiv a.packageLink{border-left:none;}
.rowSectionDiv a.packageLink span{display:inline-block; position:relative;}
/*.rowSectionDiv a.packageLink span:before{content:""; position:absolute; top:0.5rem; left:-1.8rem; width:1rem; height:1rem; border:.1rem solid #ee9591; border-radius:100%;}*/
.rowSectionDiv a.packageLink.active{border-color: transparent; color:#ee9591;}
.rowSectionDiv a.packageLink.active:before{display:none;}
/*.rowSectionDiv a.packageLink.active span:before{background:#ee9591;}*/

/******** start of pattern image *********/
.pattern_image:before{content:""; position:absolute; top:0; left:0; width:100%; height:100%; z-index:-1;
	background:linear-gradient(#f6f6f6, #fbfbfb, #ffffff);
	-moz-background:linear-gradient(#f6f6f6, #fbfbfb, #ffffff);
	-webkit-background:linear-gradient(#f6f6f6, #fbfbfb, #ffffff);
	-o-background:linear-gradient(#f6f6f6, #fbfbfb, #ffffff);}
.pattern_image:after{background-image:url(./../../../../assets/images/home/pattern.svg); background-repeat:repeat; content:""; position:absolute; top:0; left:0; width:100%; height:100%; z-index:-1;}

/******* start of addCartButton Icon css **********/
.addCartButton{position: absolute; left: 0; top: 0; background: #ee9591; color: #ffffff; cursor: pointer; padding: 0 .5rem 0 .5rem; height: 100%; line-height: 3.5rem; z-index: 1;}
.addCartButton .addCartButtonIcon{display: block; font-size: .8rem; position: relative;}
.addCartButton .addCartButtonIcon .badge{top: -.6rem; background-color: #3b4e76; width: 1rem; height: 1rem; line-height: .7rem; margin-left: -.2rem;}

/******** popup image slider **********/
div[class*="col-"].popupImageSlider ul.carousel-indicators{border: none; bottom: -1.5rem;}
div[class*="col-"].popupImageSlider ul li{background-color: rgba(0,0,0,.8); height: .3rem;}
div[class*="col-"].popupImageSlider ul li.active{background-color: #ee9591;}

/********** start of productCartPopup Item css ******************/
.cartContent .item-name{font-size: .85rem; color: #000000; transition: ease all 0.5s;}
.cartContent .item-price{font-size: .95rem; color: #000000; padding-right: 1rem;}
.cartContent .item-quantity{font-size: .8rem; color: gray;}

/********* start of userAccountBtn css *********/
.userAccountBtn{position: absolute; right: 0; top: .1rem; background: #3b4e76; color: #ffffff; border-radius: 0; padding: .98rem .5rem; z-index: 99; border-left: .01rem solid rgba(255,255,255,.3); font-size: .9rem; transition: ease all 0.5s; -webkit-transition: ease all 0.5s;}
.userAccountBtn i{font-size: .8rem;}
.userAccountBtn:hover, .userAccountBtn:focus{color: #ee9591;}
.userAccountContent{border-radius: 0; box-shadow: 0 6px 12px rgba(0, 0, 0, 0.175); -webkit-box-shadow: 0 6px 12px rgba(0, 0, 0, 0.175); border: 0rem;}
.userAccountContent a.dropdown-item {font-size: 0.9rem; padding-left: 0.8rem; padding-right: 0.8rem; padding-top: 0.5rem; padding-bottom: 0.5rem; transition: ease all 0.5s; -webkit-transition: ease all 0.5s;}
.userAccountContent a.dropdown-item:hover {background: none; color: #ee9591; text-indent:.3rem;}
.userAccountContent .loginReg_text {display: flex; align-items: center;
	background: -moz-linear-gradient(left, #8b3390 0%, #3b4e76 100%);
	background: -webkit-linear-gradient(left, #8b3390 0%, #3b4e76 100%);
	background: linear-gradient(to right, #8b3390 0%, #3b4e76 100%);}
.userAccountContent .loginReg_text a.enter-form {color: #fff; position: relative;}
.userAccountContent .loginReg_text a.enter-form:hover {color: #ee9591; text-indent:0;}
.userAccountContent .loginReg_text a.enter-form + a:before {content: "|"; position: absolute; left: -0.7rem; top: 0.55rem; font-size: 0.8rem; color: #e6e6e6;}

/*********** start of marqueeSection css *************/
.marqueeSection{position: fixed; width: 100%; left: 0; top: 0; font-size: .8rem; z-index: 10; background: #ee9591; color: #ffffff; font-weight: 500; padding: .1rem .2rem .3rem; display: flex; justify-content: center; align-items: center; flex-wrap: wrap; letter-spacing: .02rem;}
.marqueeSection small{padding: 0 .1rem;}

/******** strip add header urgest ********/
.headerSection{position: fixed; top: 0; left: 0; width: 100%; background: #fff; z-index: 9; box-shadow: 0 0 1rem rgba(0,0,0,0.1);}
.marqueeSection + .headerSection{margin-top: 1.5rem;}

/****** sideNavBar css *******/
.sideNavBar{height: calc(100vh - 5rem);}
/******* mobileBlankSpace ********/
.mobileBlankSpace{display: none;}


/* start of product_detail css */
.product_listing{padding:0 .8rem 0; margin-top:2rem;}
.product_listing .list_div{margin-bottom: 2rem; padding: 0 .5rem;}
.product_listing .list_div .product-grid3 {text-align: center; position: relative; height: 100%; background:#ffffff; box-shadow:0 0 1rem rgba(0,0,0,0.1); border:.1rem solid transparent; transition: all 0.3s ease 0s; -webkit-transition: all 0.3s ease 0s;}
.product_listing .list_div .product-grid3 .product-image3 {position: relative; padding:1rem 0;}
.product_listing .list_div .product-grid3 .product-image3 .image_part {display: block; width:100%; background:none; border:none; padding:1rem 0 0; cursor:pointer; opacity:1; transform:scale(1); transition: all 0.3s ease 0s; -webkit-transition: all 0.3s ease 0s;}
.product_listing .list_div .product-grid3 .product-image3 .image_part img {width: 20rem; height: auto; transition: all 0.3s ease 0s; -webkit-transition: all 0.3s ease 0s;}
.product_listing .list_div .product-grid3 .product-image3 .social {width: 12rem; padding: 0; margin: 0 auto; list-style: none; position: absolute; right: 0; left: 0; bottom: -3rem; transition: all 0.3s ease 0s; -webkit-transition: all 0.3s ease 0s;}
.product_listing .list_div .product-grid3 .product-image3 .social li {display: inline-block;}
.product_listing .list_div .product-grid3 .product-image3 .social li .btn {color: #3b4e76; background: #fff; font-size: 1.1rem; line-height: 3.3rem; width: 3.5rem; height: 3.5rem; border: 0.1rem solid rgba(0, 0, 0, 0.1); padding:0 .2rem; border-radius: 50%; display: block; margin-left: 0.2rem; margin-right: 0.2rem; margin-top: 0; margin-bottom: 0; transition: all 0.3s ease 0s; -webkit-transition: all 0.3s ease 0s;}
.product_listing .list_div .product-grid3 .product-image3 .social li .btn:hover {background: #3b4e76; color: #ee9591;}

/**************** for images label use css *************************/
.product_listing .list_div .product-grid3 .product-content {z-index: -1; padding: 1.5rem 1rem 0; text-align: center;}
.product_listing .list_div .product-grid3 .product-content .title {font-size: 1rem; margin: 0 0 0.6rem; border-top: 0.1rem solid rgba(0, 0, 0, 0.1); padding-top: 2.3rem; transition: all 0.3s ease 0s; -webkit-transition: all 0.3s ease 0s;}
.product_listing .list_div .product-grid3 .product-content .title .title_content {color: rgba(0, 0, 0, 0.7); display:block; cursor:pointer;}
.product_listing .list_div .product-grid3 .product-content .product_price {color: #3b4e76; font-size: 1.4rem; font-weight: 600; margin-right: .05rem; display: block; padding:.5rem 0 1rem;}
.product_listing .list_div .product-grid3 .product-content .product_price span {color: #ee9591; font-size: 0.9rem; font-weight: 300; letter-spacing: 0; text-align: left; display: inline-block; margin-top: -.05rem; position: relative; overflow: hidden; top: .3rem;}
.product_listing .list_div .product-grid3 .product-content .product_price span:before{content: ""; position: absolute; top: 0.7rem; left: 0; height: .09rem; width: 200%; background: #3b4e76; z-index: 1;}
.product_listing .list_div .product-grid3 .product-content .product_price b{text-transform: uppercase; color: #ee9591; font-size: 1rem; font-weight: 500; margin: 0 .3rem;}
.product_listing .list_div .product-grid3 .product-content .product_size {display: block; color: rgba(0, 0, 0, 0.6); font-size: 0.85rem; font-weight: 300; position: relative; top: .2rem;}
.product_listing .list_div .product-grid3 .product-content .product_size mark{padding:.2em .5rem; background-color:#fbe1df;}
.product_listing .list_div .product-grid3 .product-content .product_sold {display: none; color: rgba(0, 0, 0, 0.6); font-size: 0.8rem; margin-top: 0.6rem; width: 100%; text-align: center; position: absolute; left: 0; bottom: 0; background: rgba(0, 0, 0, 0.05); padding: 0.1rem 0.4rem;}
.product_listing .list_div .product-grid3 .product-content .product_sold span {font-weight: bolder;}

/************ start of hover ****************/
.product_listing .list_div .product-grid3:hover{box-shadow:0 0 2rem rgba(0,0,0,0.2);}

.product_listing .list_div .product-grid3:hover .product-content .title .title_content,
.product_listing .list_div .product-grid3:focus .product-content .title .title_content,
.product_listing .list_div .product-grid3:active .product-content .title .title_content {color: #3b4e76;}

/************ start of add or remove class **************/
.product_listing .list_div .product-grid3 .product-image3 .social li .btn.remove{display:none;}
.product_listing .list_div .product-grid3 .product-image3 .social li .btn.go-cart-btn{display:none;}
.product_listing .list_div .product-grid3 .product-image3 .social li .btn.go-cart-btn.active{display:inline-block; background: #3b4e76; color: #ee9591;}
.product_listing .list_div .product-grid3.active{border-color:#ee9591;}
.product_listing .list_div .product-grid3.active .product-image3 .social li:first-child{position:relative; top:.15rem;}
.product_listing .list_div .product-grid3.active .product-content .title a{color:#3b4e76;}
/************ end of add or remove class **************/

/********** start of productTags css ***********/
.productTags{position: absolute; top: -.1rem; left: -.1rem; z-index: 1; background: #3b4e76; padding: .1rem 1rem .4rem; text-align: center; text-align: center; line-height: .9;}
.productTags span{color: #ee9591; font-size: 10px; text-transform: uppercase; letter-spacing: .05rem;}

/******** start of genderIcon css ***********/
.genderIcon{display: flex; flex-direction: row; align-items: center; color: #3b4e76; padding: 0; position: absolute; top: 0.1rem; right: .6rem; z-index: 1;}
.genderIcon .iconGender img{width: .8rem; height: 1rem;}
.genderIcon .iconText{font-size: .8rem; display: inline-flex; padding-top: .1rem; padding-left: .2rem; text-transform: capitalize;}

/******* start of saleEndsContent *******/
.saleEndsContent{position: fixed; width: 100%; left: 0; top: 0; font-size: .8rem; z-index: 10; background: #ee9591; box-shadow: 0 0 .5rem rgba(0,0,0,.1); font-weight: 500; padding: .5rem .2rem; display: flex; justify-content: center; align-items: center; flex-wrap: wrap; letter-spacing: .02rem; text-align: center;}
.saleEndsContent h5{font-size: .8rem; color: #3b4e76; margin: 0; letter-spacing: .01rem;}
.saleEndsContent h5 span{font-size: 1.8rem; color: #ffffff; font-weight: bold; display: block; padding-top: .1rem;}
.saleEndsContent h5 span b{font-size: 0.8rem; color: #3b4e76; padding-left: .2rem; font-weight: normal;}

/******** start of sale then header Alignment **********/
.saleEndsContent + section{margin-top: 5rem;}
.saleEndsContent + section .headerSection{margin-top: 4.2rem;}


/****************** start of media css *******************/

@media (max-width: 767.98px) {
	/* start of header_section css */
	.rowSectionDiv a, .rowSectionDiv .moreIfoText{font-size:.9rem; letter-spacing: .01rem; padding: .8rem 1.5rem;}
	.rowSectionDiv a.categoryDiv{padding: .8rem .5rem; font-size:.75rem;}
	.rowSectionDiv a.logoLink span{position:relative; top:0; left:0;}
	.rowSectionDiv a.logoLink img{width:2.9rem;}
	
	/******* start of addCartButton Icon css **********/
	.addCartButton{position: fixed; top: auto; bottom: 0; height: auto; line-height: 2.95rem; width: 12%; flex: 0 0 12%; padding: 0; text-align: center;}
	.addCartButton .addCartButtonIcon{font-size: .7rem;}
	
	/************* start of footer_section css **********/
	/*.rowSectionDiv a.packageLink span:before{top:0.2rem; left:-1.2rem; width:.7rem; height:.77rem;}*/
	.rowSectionDiv a.borderTopGold, .rowSectionDiv .moreIfoText.borderTopGold{border-top:.1rem solid #ee9591;}
	.rowSectionDiv a.cartPriceSpace{padding-left: 3rem;}
	.footerDiv .rowSectionDiv + .rowSectionDiv{border-left: .01rem solid rgba(255,255,255,.3);}
	.rowSectionDiv a.categoryDiv.cartPriceSpace.active:after{left: 65%;}
	
	/*.singleMobileWidth, .packageMobileWidth{max-width: 42.5%; flex: 0 0 42.5%;}*/
	
	/********* start of userAccountBtn css *********/
	.userAccountBtn{top: auto; bottom: 5rem; padding: .88rem .5rem; position: fixed; border-top-left-radius: .6rem; border-bottom-left-radius: .6rem;}	
	
	/******* mobileBlankSpace ********/
	.mobileBlankSpace{display: block; height: calc(100vh - 13rem); width: .01rem;}
	
	/******* start of saleEndsContent *******/
	.saleEndsContent{padding: .3rem .2rem .5rem;}
	.saleEndsContent h5{letter-spacing: 0; display: flex; align-items: center; justify-content: center;}
	.saleEndsContent h5 span{font-size: 1.5rem; padding: 0 0 .1rem 1rem; display: flex; align-items: center; position: relative; margin-left: .5rem;}
	.saleEndsContent h5 span:before{content: ":"; position: absolute; left: .2rem; top: .3rem; font-size: 1rem; color: #3b4e76;}
	.saleEndsContent h5 span b{padding: 0 .6rem 0 .3rem;}

	/******** start of sale then header Alignment **********/
	.saleEndsContent + section{margin-top: 2.5rem;}
	.saleEndsContent + section .headerSection{margin-top: 2.6rem;}
}